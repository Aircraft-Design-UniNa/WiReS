FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      topoSetDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
 
actions
(
    {
       name      global;
       type      cellSet;
       action    new;
       source    cylinderToCell;
       sourceInfo
       {
          p1       (-10000 0 0);   // start point on cylinder axis
          p2       (10000 0 0);   // end point on cylinder axis
          radius   10000;
       }
    }
);
